{
  "name": "@mohssineaboutaj/grid-system",
  "version": "1.1.0",
  "private": false,
  "description": "A simple grid system with 4 screen sizes (sm, md, lg, xl) based on bootstrap media query sizes to make your design responsive with maximum media resolutions",
  "main": "gulpfile.js",
  "scripts": {
    "start": "gulp",
    "dev": "gulp",
    "build:css": "rm -fr css/ && gulp build-css-task",
    "build": "rm -fr css/ && gulp build-docs-task && cp css/*.css docs/css/",
    "test": "node ./node_modules/.bin/jasmine JASMINE_CONFIG_PATH=jasmine.json",
    "deploy": "gh-pages -d docs"
  },
  "keywords": [
    "gulp",
    "sass",
    "scss",
    "design",
    "responsive",
    "grid",
    "grid system",
    "flexbox"
  ],
  "author": "Mohssine Aboutaj",
  "license": "MIT",
  "browserslist": [
    "last 2 version",
    "> 2%"
  ],
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@fortawesome/fontawesome-free": "^5.11.2",
    "browser-sync": "^2.26.7",
    "gh-pages": "^2.1.1",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^4.2.0",
    "gulp-cli": "^2.2.0",
    "gulp-concat": "^2.6.1",
    "gulp-concat-css": "^3.1.0",
    "gulp-pug": "^4.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "highlightjs": "^9.16.2",
    "jasmine": "^3.5.0",
    "js-cookie": "^2.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohssineAboutaj/grid-system.git"
  },
  "bugs": {
    "url": "https://github.com/mohssineAboutaj/grid-system/issues"
  },
  "homepage": "https://mohssineAboutaj.github.io/grid-system",
  "dependencies": {}
}
