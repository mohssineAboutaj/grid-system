{
  "name": "@mohssineaboutaj/grid-system",
  "version": "1.0.51",
  "private": false,
  "description": "A simple grid system with 4 screen sizes (sm, md, lg, xl) based on bootstrap sizes to make your design responsive with maximum media resolutions",
  "main": "gulpfile.js",
  "scripts": {
    "start": "gulp",
    "dev": "gulp",
    "build:css": "rm -fr css/ && gulp build-css-task",
    "build": "gulp build-docs-task && cp css/*.css docs/css/",
    "test": "node ./node_modules/.bin/jasmine JASMINE_CONFIG_PATH=jasmine.json",
    "deploy": "gh-pages -d docs"
  },
  "keywords": [
    "gulp",
    "sass",
    "scss",
    "responsive",
    "grid",
    "grid system"
  ],
  "author": "Mohssine Aboutaj",
  "license": "MIT",
  "browserslist": [
    "last 2 version",
    "> 2%"
  ],
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "gulp": "3.9.0",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-clean-css": "^4.2.0",
    "gulp-cli": "^2.2.0",
    "gulp-concat": "^2.6.1",
    "gulp-concat-css": "^3.1.0",
    "gulp-pug": "^4.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "jasmine": "^3.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohssineAboutaj/grid-system.git"
  },
  "bugs": {
    "url": "https://github.com/mohssineAboutaj/grid-system/issues"
  },
  "homepage": "https://mohssineAboutaj.github.io/grid-system",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.11.2",
    "gh-pages": "^2.1.1",
    "highlightjs": "^9.16.2",
    "js-cookie": "^2.2.1"
  }
}
